{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "module": "commonjs",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "target": "es6",
    "lib": ["es6", "dom", "es2015", "es2017"],
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "baseUrl": "."
  }
}
